{
  "expo": {
    "scheme": "aichat501",
    "name": "AiChat501",
    "slug": "AiChat501",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/iconFinal.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow MIRACLE AI to access your microphone.",
          "speechRecognitionPermission": "Allow MIRACLE AI to securely transcribe your Audio input."
        }
      ],
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow MIRACLE AI to access your microphone.",
          "speechRecognitionPermission": "Allow MIRACLE AI to securely transcribe your Audio input."
        }
      ]
    ],
    "splash": {
      "image": "./assets/icons/screen.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wired87.AiChat501",
      "config": {
      "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/screen.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wired87.AiChat501",
      "splash": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icons/iconFinal.png"
    },
    "extra": {
      "eas": {
        "projectId": "5f9f8766-f604-4b83-b922-ace89299d73e"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2225753085204049~1255976039"
  }
}
